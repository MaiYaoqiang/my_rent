/*通用属性和整体属性*/
	body{
		font:16px "microsoft yahei",Arial,Helvetica,sans-serif;
		color:#A3A3A3;
		margin:0;
		padding:0;
	}
	a{
		color:#27DA93;
		text-decoration:none;
	}
	h1,h2,h3,h4,h5,h6,p,ul,ol,dl,dd{
		margin:0;
		padding:0;
		list-style:none;
	}
	img{
		vertical-align:bottom;
	}
	.lf{float:left;}
	.rt{float:right;}
	.clear{clear:both;}
	.container{
		width:1170px;
		margin:0 auto;
		min-height:70px;
	}
	.container:before{
		content:"";
		display:table;
	}
	.container:after{
		content:"";
		display:block;
		clear:both;
	}
/**************************************/
/********头部1***********/
	.header1{
		background:#27DA93;
		line-height:70px;
		color:white;
	}
	.header1 .container{
		padding:0 20px;
		width:1130px;
	}
	.header1>div>span{
		font-size:30px;
		font-weight:bold;
	}
	.header1 ul li{
		float:left;
		margin:0 10px;
	}
	.header1 a{
		color:white;
	}
	.header1 ul li span.phone:before{
		content: "\e145";
	}
/**************************/
/*********头部2**************/
	.header2{
		height:550px;
		background:url(../Images/4.jpg) no-repeat ;
		background-size:cover;
		position:relative;
	}
	.header2_us{
		position:absolute;
		top:43%;
	}
	.header2_us{
		font-size:42px;
		color:#fff;
	}
	h3 span{
		border-bottom:5px solid #27DA93;	
	}
	.header2_us>span{
		font-size:28px;
	}
/**********************/
/*********导航************/
	.nav .container{
		height:96px;
		position:relative;
		box-shadow:0 0 4px rgba(164,164,164,.5);
	}
	.nav .container li{
		float:left;
		width:14.2%;
		height:96px;
		position:relative;
		border-right:1px solid #EBEBEB;
	}
	
	.nav .container li a div{
		width:100%;
		height:126px;
		position:absolute;
		bottom:0;
		background:#fff;
		text-align:center;
		border-right:1px solid #ebebeb;
		color:#919090;
		transition:height 0.3s linear;
		
	}
	.nav .container li a div:hover{
		height:170px;
	}
	.nav .container li a div:hover p{
		color:#6DE6B6;
	}
	.nav .container li a div p{
		color:#919090;
		font-weight:bold;
	}
	.nav .container li i{
		display:inline-block;
		width:34px;
		height:34px;
		background:url(../images/img-sprite.png) no-repeat;
		margin-top:30px;
	}
	.nav .container li:nth-child(1) i{
		background-position:-3px -42px;
	}
	.nav .container li:nth-child(1) div:hover i{
		background-position:-3px -86px;
	}
	.nav .container li:nth-child(2) i{
		background-position:-37px -39px;
	}
	.nav .container li:nth-child(2) div:hover i{
		background-position:-37px -77px;
	}
	.nav .container li:nth-child(3) i{
		background-position:-73px -42px;
	}
	.nav .container li:nth-child(3) div:hover i{
		background-position:-73px -79px;
	}
	.nav .container li:nth-child(4) i{
		background-position:-144px -41px;
	}
	.nav .container li:nth-child(4) div:hover i{
		background-position:-144px -82px;
	}
	.nav .container li:nth-child(5) i{
		background-position:-176px -42px;
	}
	.nav .container li:nth-child(5) div:hover i{
		background-position:-176px -81px;
	}
	.nav .container li:nth-child(6) i{
		background-position:-213px -39px;
	}
	.nav .container li:nth-child(6) div:hover i{
		background-position:-213px -77px;
	}
	.nav .container li:nth-child(7) i{
		background-position:-248px -40px;
	}
	.nav .container li:nth-child(7) div:hover i{
		background-position:-248px -79px;
	}
	.nav .container li:nth-child(7) div{
		width:167px;
	}
/**************************/
/******最流行*******/
	.main_popular{
		height:600px;
		background:url(../Images/back1.jpg);
		background-size:cover;
	}
	.word{
		margin:64px auto;
		font-size:28px;
		color:#fff;
		text-align:center;
	}
	.word i{
		border:1px solid #fff;
		display:inline-block;
		width:20%;
		margin:20px;
	}
	.photo>div{
		margin:0 20px;
		height:380px;
		width:350px;
		overflow:hidden;
		position:relative;
		float:left;
	}
	.photo>div img{
		width:350px;
		position:absolute;
		transition:transform 0.5s linear;
	}
	.photo>div img:hover{
		transform:scale(1.1);
	}
	.photo>div>div{
		width:318px;
		padding:16px;
		background:white;
		height:47px;
		font-size:14px;
		position:absolute;
		top:263px;
	}
	a.to_green{
		color:#27DA93;
		font-size:16px;
		transition:color 0.3s linear; 
		margin-bottom:10px;
		display:inline-block;
	}
	a.to_green:hover{
		color:#000;
	}
	.photo a:last-child{
		position:absolute;
		top:30%;
		right:0px;
		padding:0 10px;
		background:#27DA93;
		color:white;
		text-align:center;
		line-height:250%;
	}
/****************************/
/***********服务*************/
	.black_word{
		color:#000;
		margin:64px auto;
		font-size:30px;
		text-align:center;
	}
	.black_word i{
		border:1px solid #000;
		display:inline-block;
		width:20%;
		margin:20px;
	}
	.main_service ul{
		margin-bottom:30px;
	}
	.main_service ul li{
		width:50%;
		float:left;
		margin-bottom:40px;
	}
	.main_service ul li a{
		width:74px;
		height:72px;
		border:3px solid #27DA93;
		border-radius:50%;
		text-align:center;
		line-height:55px;
		position:relative;
	}
	.main_service ul li a span{
		width:100%;
		height:100%;
		position:absolute;
		top:0;
		left:0;
		transition:all 0.3s linear;
		border-radius:50%;
		opacity:0.3;
	}
	.main_service ul li a:hover span{
		box-shadow:0 0 2px 5px #000;
		opacity:0.1;
		transform: scale(1.2);
	}
	
	.main_service ul li>div{
		width:415px;
		float:right;
		margin-right:30px;
	}
	.main_service ul li p{
		font-size:18px;;
		color:#27DA93;
		font-weight:bold;
	}
	.main_service ul li span{
		font-size:14px;
		line-height:24px;
	}
/****************************/
/***********特点**************/
	.main_features{
		height:400px;
		background:url(../Images/ab.jpg) no-repeat center;
		background-size:cover;
		margin-top:30px;
	}
	.main_features>div>div{
		width:335px;
		height:258px;
		background:#27DA93;
		float:right;
		margin:78px 20px 0 0;
		padding:32px;
	}
	.main_features>div>div p{
		font-size:40px;
		font-weight:bold;
		color:white;
		margin-bottom:20px;
	}
	.main_features>div>div span{
		font-size:14px;
		color:white;
		line-height:30px;
	}
	a.white_through{
		position:relative;
		background:#27DA93;
		border:1px solid #fff;
		margin-top:30px;
		display:inline-block;
	}
	.main_features>div>div a.white_through>span{
		line-height:24px;
	}
	a.white_through>span{
		color:white;
		padding:2px 10px;
		position:relative;
		transition:all 0.3s linear;
		display:inline-block;
		line-height:24px;
	}
	a.white_through:hover>span{
		color:#27DA93;
	}
	a.white_through:before{
		content:"";
		width:0%;
		height:100%;
		background:#fff;
		transition:all 0.3s linear;
		position:absolute;
		top:0;
		left:0;
	}
	a.white_through:hover:before{
		width:100%;
	}
/****************************/
/**********大手机***************/
	.main_phone{
		height:482px;
		padding:80px 0;
	}
	.main_phone container{
		padding:0 15px;
	}
	.main_phone .phone{
		margin:0 140px;
		padding:0 15px;
	}
	.main_phone .phone_text{
		width:540px;
		padding:0 15px;
	}
	.main_phone .phone_text .black_word{
		text-align:left;
		margin:0;
		font-size:40px;
	}
	.main_phone .phone_text li{
		width:378px;
		height:80px;
		font-size:14px;
		line-height:24px;
	}
	.main_phone .phone_text li p{
		font-size:20px;
		margin:15px 0 10px;
	}
	a.green_through{
		position:relative;
		background:#fff;
		border:1px solid #27DA93;
		margin-top:30px;
		display:inline-block;
	}
	a.green_through>span{
		color:#27DA93;
		padding:2px 10px;
		display:inline-block;
		position:relative;
		transition:all 0.3s linear;
		line-height:24px;
	}
	a.green_through:hover>span{
		color:#fff;
	}
	a.green_through:before{
		content:"";
		width:0%;
		height:100%;
		background:#27DA93;
		transition:all 0.3s linear;
		position:absolute;
		top:0;
		left:0;
	}
	a.green_through:hover:before{
		width:100%;
	}
/******************************/
/************产品画廊***************/
	.main_gallery{
		background:#F7F7F7;
		height:650px;
	}
	.main_gallery .black_word{
		margin:30px 0;
	}
	.main_gallery li {
		width:253px;
		border:1px solid #AEAEAE;
		position:relative;
		float:left;
		overflow:hidden;
		height:389px;
	}
	.main_gallery li a img{
		width:253px;
		height:190px;
		vertical-align:b;
		position:absolute;
		top:0;
		left:0;
	}
	.main_gallery li .gallery_message{
		position:relative;
		top:189px;
		left:0;
		padding:24px;
		height:152px;
		background:#fff;
	}
	.main_gallery li .gallery_message>div{
		margin-bottom:15px;
	}
	.main_gallery li .gallery_message>div i{
		width:40px;
		height:40px;
		background:url(../images/star1.png) 0px 0px no-repeat;
		color:#fff;
		line-height:40px;
		text-align:center;
	}
	.main_gallery li .gallery_message>div>div{
		margin-left:16px;

	}
	.main_gallery li .gallery_message>div>div b{
		color:black;
		font-size:16px;
	}
	.gallery_message span{
		font-size:12px;
	}
	.gallery_message p{
		font-size:12px;
		margin-top:5px;
		color:#000;
	}
	.gallery_message a.green_through>span{
		font-size:16px;
	}
	.gallery_message a.green_through{
		bottom: 24px;
		left: 24px;
		position:absolute;
	}
/*****************************/